<style>    
  @font-face
  {
    font-family: 'dejavu';
    font-style: normal;
    font-weight: normal;
    src: url(<%= (@export ? "assets/" : "/template/#{@template}/") + 'fonts/bpg_dejavusans_2.ttf'%>);      
  }

  @font-face
  {
    font-family: 'dejavu-bold';
    font-style: normal;
    font-weight: normal;
    src: url(<%= (@export ? "assets/" : "/template/#{@template}/") + 'fonts/bpg_dejavusans_bold.ttf'%>);    
  }

  @font-face
  {
    font-family: 'shavshvebi';
    font-style: normal;
    font-weight: normal;
    src: url(<%= (@export ? "assets/" : "/template/#{@template}/")  + 'fonts/shavshvebi.ttf'%>);    
  }
  body {
    font: 17px/18px 'dejavu-bold';
    margin: 0;
    padding: 0;
    background-color:#fff;
    -webkit-font-smoothing: antialiased;    
  }
  a 
  {    
    text-decoration: none;
  }
  a:hover {    
     cursor:pointer;
     color: #004276;
     text-decoration: none;
  }
  p
  {
     font-family:'dejavu' !important; 
     -webkit-font-smoothing: antialiased;  
     font-size: 13px;
     line-height: 21px;
  }
  a:hover {
    text-decoration: underline;
  }

  .article {
    /*border-top: solid 1px #000;*/
  }

  .section.column {
    font-family:'dejavu-bold';
    -webkit-font-smoothing: antialiased;  
    margin-top: 100px;
  }
  .section.column h1, .section.column h2, .section.column h3
  , .section.column h4, .section.column h5, .section.column h6{
    font-family:'dejavu-bold';
    -webkit-font-smoothing: antialiased;  
  }


  .section:after {
    content: "";
    clear: left;
    display: block;
  }

  .section--last {
    margin-bottom: 130px;
  }

  .section--credits {
    color: #999;
    font-style: oblique;
  }

  .section--credits a {
    color: #444;
  }

  .column {
    margin-left: auto;
    margin-right: auto;
    max-width: 580px;
  }

  .column em{
    font-style: italic;
  }

  .column ul, .column ol, .column ul ul, .column ol ol,
  .column ul ol, .column ol ul {
	  padding-left: 2em;
  }

  .column ul li {
	  list-style-type:disc;
	  line-height: 1.5;
  }
  .column ol li {
	  list-style-type:decimal;
	  line-height: 1.5;
  }
  
  .column a{
    text-decoration: underline;
  }
  
  .column blockquote {
    padding: 0 0 0 15px;
    margin: 0 0 20px;
    border-left: 5px solid #eeeeee;
  }

  .column img{
    width: auto;
    margin: 0 10px;
  }
  
  .section h3 {
    margin-right: 10px;
    margin-bottom: 20px;
  }

  .scroll-prompt {
    position: absolute;
    bottom: 80px;
    line-height: 48px;
    text-align: center;
    color: white;
    font-size: 14px;
    opacity: 1;
    font-weight: bold;
    text-transform: uppercase;
    z-index: 3;
    margin:0px auto;    
    text-shadow: 0 1px 2px #000;
/*    background-image: url(<%= (@export ? "assets/png/" : "/assets/png/") + 'scroll.png'%>);      
    background-position: 50% 0%;
    background-repeat: no-repeat; */
    width:100%;
  }

  .scroll-prompt svg {
    fill: none;    
    stroke-width: 2px;       
    position:relative;
    top:28px;
    animation:arrows infinite;
    animation-duration:0.9s;
    animation-direction:alternate-reverse;

    /*  Safari and Chrome   */
    -webkit-animation:arrows infinite;
    -webkit-animation-duration:0.9s;
    -webkit-animation-direction:alternate-reverse;
}
  @-webkit-keyframes arrows {
    from {
      top:28px;
    }
    to {
      top:38px;
    }
  }
  @keyframes arrows {
    from {
      top:28px;
    }
    to {    
      top:38px;
    }
  }
  .dropcap {
    float: left;
    font: bold 53px/70px "Helvetica Neue", Helvetica, Arial, sans-serif;
    margin: -7px 10px 0 0;
    height: 1em;
  }

  .headline,
  .section h3, .slideshow-headline {
    font: 200 30px/40px shavshvebi, Georgia, serif;
  }

  .headline {
    font-size: 45px;
    line-height: 60px;
    margin-bottom: 15px;
  }
  .slideshow-headline
  {
    font-size: 45px;
    line-height: 60px;
    margin: 80px 0px 15px 0px;
  }
  .byline-rule {
    /*border-top: 2px solid #000;*/
    width: 120px;
  }

  .byline {
    font: 12px/2em dejavu,"Helvetica Neue", Helvetica, Arial, sans-serif;
    opacity: .5;
  }

  .byline .author {
    text-transform: uppercase;
    white-space: nowrap;    
  }

  .byline--first {
    margin-bottom: 14px;
    opacity: 1;
    font-weight: 700;
    padding-top: 23px;
  }

  @media (max-width: 704px) {
    .column {
      margin: auto 5%;
      max-width: 100%;
    }
  }

  @media only screen and (min-device-width: 320px) and (max-device-width: 480px) {
    .ios body {
      font-size: 16px;
      line-height: 26px;
    }

    .ios .column {
      margin: auto 8%;
    }

    .ios .section.column {
      margin-top: 50px;
    }

    .ios .navigation {
      background-color: #000;
      position: static;
    }

    .section h4 {
      font-size: 24px;
      text-align: center;
      margin: 10px 30px;
    }

    .section h3 {
      font-size: 48px;
      text-align: center;
      margin: 10px 30px 30px 30px;
    }
  }

  .section.column img.image  {
    margin: 0;
    max-width: 100%;
  }

  .image .caption {
    font: 12px/1.4em "Helvetica Neue", Helvetica, Arial, sans-serif;
    clear: left;
    color: #999;
  }

  @media (min-width: 1200px) {
    .image--diptych {
      width: 200%;
      margin-left: -50%;
    }

    .image--diptych img {
      position: relative;
      width: 50%;
      float: left;
      left: 5px;
    }

    .image--diptych .caption {
      margin-left: -5px;
      padding-top: .7em;
    }

    .image--diptych img:first-child {
      left: -5px;
    }
  }
  .video {
    font: bold 17px/30px "Helvetica Neue", Helvetica, Arial, sans-serif;
    position: relative;
    width: 100%;
    overflow: hidden;
  }

  .video-container {
    float: left;
    height: 100%;
    overflow: hidden;
    position: relative;
    width: 100%;
  }

  .video-container video,
  .video-container img {
    width: 100%;
    position: absolute;
  }

  .video-caption {   
    padding-bottom: 20px;    
    left:5%;    

  }

  .video-source {
    font: 12px/1.4em "Helvetica Neue", Helvetica, Arial, sans-serif;
    color: #bdbdbd;
    position: absolute;
    bottom: 7%;
    right: 7%;
    z-index: 3;
  }

  .column .video {
    border: solid 8px #f0f0f0;
    box-sizing: border-box;
    margin: 40px -10%;
    width: 120%;
  }

  .video-sequence:first-child {
    margin-top: 40px;
  }

  .video-sequence .video {
    min-height: 54vw;
  }

  .video-sequence-indicator {
    font-family: arial;
    position: absolute;
    line-height: 1.4em;
    top: 50%;
    right: 20px;
    color: #fff;
    white-space: pre;
  }

  .video {
    float: left;
  }

  .video + p {
    clear: left;
  }

  .video .video-caption {
    position: absolute;    
    bottom: 10%;
    left: 7%;    
    z-index: 3;
    color:#fff;        
    font-weight:normal;
    width: 360px;
    font-family:dejavu-bold;
    font-size:16px;
    -webkit-font-smoothing: antialiased;  
    line-height: 24px;
    text-shadow: 0 0px 45px rgba(0, 0, 0, 0.22);
    padding: 20px 0;
    letter-spacing: -0.2px;
  }

 .video-sequence:first-of-type .video:first-of-type
 , .video-sequence:last-of-type .video:last-of-type{
    padding-bottom:0px !important;
  }

  .video .video-caption--long {
    width: 400px;
    bottom: 12.44%;
  }

  .video .video-caption--right {
    right: 7%;
    left: inherit;
    text-align: left;
  }

  .video .video-caption--center {
    left: inherit;
    text-align: center;
    width: 50%;
    margin: 0 25%;
  }

  .video .video-caption--head {
    top: 0;
    left: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    background-color: transparent;

  }


  .video-caption--head .video-headline {
    position: absolute;
    top: 50%;
    color: #fff;
 /*   
    left: 7%;*/
    text-shadow: none;
    left: inherit;
    text-align: center;
    width: 50%;
    margin: 0 25%;
    line-height: 12px;
    /*border-bottom: solid 2px black;  */
    /*padding-bottom: 35px;*/
  }
    .headline-line1
    {
        font-size: 213px;        
    }
   .headline-line2
    {
        font-size: 83px;
        line-height: 153px;
    }

  .video-caption--head .video-summary {
    position: absolute;
    bottom: 10%;
    left: 7%;    
    z-index: 3;
    color:#fff;        
    font-weight:normal;
    width: 340px;
    font-family:dejavu-bold;
    font-size:16px;
    -webkit-font-smoothing: antialiased;  
    line-height: 24px;
    text-shadow: 0 0px 45px rgba(0, 0, 0, 0.22);
    padding: 20px 0;
  } 
  .ios .video-rule {
    display:none;
  }
  .ios .video-caption--right
  {
    left: 7%;   
    text-align: left;
  
  }
  .ios .video-sequence .video:last-of-type .video-caption {
    bottom: 65px;
  }
  .ios .align-right
  {
    bottom: 65px;
    width: 260px;
    left: 50px;
    text-align: left;
  }
