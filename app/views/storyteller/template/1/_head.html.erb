    
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
    <meta name="viewport" content="initial-scale=1,width=device-width,height=device-height,user-scalable=no">       
    <title><%= t('app.common.app_name') %> - <%= @story.title %></title>    
    <!-- Le HTML5 shim, for IE6-8 support of HTML elements -->
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js" type="text/javascript"></script>
    <![endif]-->    
    
		  <meta property="og:title" content="<%= ("#{@story.title} | #{t('app.common.app_name')}").html_safe %>" />
		  <% if false %>
        <meta name="description" content="">
    		<meta property="og:description" content="" />
      <% end %>
		  <meta property="og:type" content="non_profit" />
		  <meta property="og:url" content="<%= current_url %>" />
		  <% if @story && @story.id && @story.asset_exists? %>
			  <meta property="og:image" content="<%= full_url(@story.asset.asset.url(:"250x250", false)) %>" />
      <% else %>
			  <!--meta property="og:image" content="" /-->
		  <% end %>
		  <meta property="og:site_name" content="<%= t('app.common.app_name') %>" />
		  <% if Rails.env.production? %>
  		  <meta property="fb:app_id" content="<%= ENV['STORY_BUILDER_FACEBOOK_APP_ID'] %>" />
		  <% end %>
    
        
    <% if @export %>
        <link href="favicon.ico" rel="shortcut icon" type="image/vnd.microsoft.icon">
    <% else %>
        <%= stylesheet_link_tag "application", :media => "all" %>               
        <%= favicon_link_tag '/favicon.ico', :rel => 'shortcut icon' %>           
    <% end %>
    <%= render partial: "storyteller/template/#{@template}/styles"%>
    
