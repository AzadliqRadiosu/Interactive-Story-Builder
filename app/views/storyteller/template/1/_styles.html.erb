  {
    width: 100%;
    text-align: right;
    padding-right: 25px;
  }

  .column .video {
    border:8px solid #f0f0f0;
    box-sizing: border-box;
    margin: 40px -10%;
    width: 120%;
  }

  .video-sequence:first-child {
    margin-top: 40px;
  }

  .video-sequence .video {
    min-height: 56.25vw;
  }

  .video-sequence-indicator {
    font-family: arial;
    position: absolute;
    line-height: 1.4em;
    top: 50%;
    right: 20px;
    color: #fff;
    white-space: pre;
  }

  .video {
    float: left;
  }

  .video + p {
    clear: left;
  }
  .video .video-caption
  {
    z-index: 3;
    color: #fff;
    
  }
  .video.infobox-floating .video-caption
  {
    position: absolute;
    left: 7%;
    bottom: 20%;
    width:350px;
    padding:20px 0px;
  }
  .video-container.infobox-fixed .video-caption
  {
    display:table;
    width:100%;
