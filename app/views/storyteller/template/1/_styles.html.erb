    padding: 0px;
    z-index: 3;
  }
  .video-container.infobox-fixed .infobox .video-caption .caption
  {
    display:table-cell;
    vertical-align:middle;

  }
  .video.infobox-floating .video-caption--long {
    width: 400px;
    bottom: 12.44%;
  }

  .video.infobox-floating .video-caption--right {
    right: 7%;
    left: inherit;
    text-align: left;
  }
  .video-container.infobox-fixed .video-caption--right
  {
    text-align:right;
  }

  .video.infobox-floating .video-caption--center {
    left: inherit;
    text-align: center;
    width: 50%;
    margin: 0 25%;
  }  
  .video-container.infobox-fixed .video-caption--center
  {
    text-align:center;
  }

 .video-headline {    
    top: 0;
    left: 0;
    padding: 0;
    width: 100%;
    height: 100%;  
  }
  .video-headline .headline
  {    
    position: absolute;
    bottom: 10%;
    color: #fff;
    left: 10%;
    font-size: 4vmin;
    line-height: 55px;
    z-index: 3;
  }

@media only screen and (min-width: 1200px) and (max-width: 1440px) and (min-height: 500px) and (max-height: 800px) {
  .video-caption--head .video-headline {
    top: 20%;
  }

  .video-caption--head .video-summary {
    bottom: 20%;
  }
}

